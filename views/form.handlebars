
<div class="container">
  <form class="form-inline" method="post" action="/postinputurl">
    <div class="form-group">
    <label for="inputurl">URL: </label>
    <input name="inputurl" type="text" class="form-control" id="inputurl">  
      </div>
      
  <button type="submit" class="btn btn-default">Submit</button>
  </form>
    <div id="shortenedurl"><h1>Shortened Url</h1><a href="http://localhost:3000/{{res}}">http://localhost:3000/{{res}}</a></div>
  <div class="original">
      <h1>Original URL:</h1>
      {{#each links as |link|}} 
      <div> {{link}}</div>
      {{/each}}
     
      <h1>Times Visited:</h1>
      {{#each counts as |count|}} 
      <div>{{count}}</div>
      {{/each}}
</div>
</div>
    
